{
  "manifest_version": 3,
  "name": "MIS REDES",
  "version": "1.0",
  "description": "Una extensi√≥n de Chrome en donde tengo todas mis redes",
  "permissions": [
    "storage",
    "downloads",
    "webNavigation",
    "webRequest",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon/icon16.png",
      "48": "icon/icon48.png",
      "128": "icon/icon128.png"
    }
  },
  "icons": {
    "16": "icon/icon16.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "version_name": "1.0"
}



